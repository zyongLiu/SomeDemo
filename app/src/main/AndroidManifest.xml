<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.testsurfaceview">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".crash.CrashApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.PulginActivity" />
        <activity android:name=".activity.SurfaceViewActivity" />
        <activity android:name=".activity.TouchActivity" />
        <activity android:name=".activity.RetrofitActivity"/>
        <activity android:name=".activity.RxJavaActivity"/>
        <activity android:name=".activity.RxReActivity" />
        <activity android:name=".activity.OkhttpActivity"/>
        <activity android:name=".activity.RulersViewActivity"/>
        <activity android:name=".activity.ScrollActivitiy"/>
        <activity android:name=".activity.VerticalViewPagerActivity"/>
        <activity android:name=".activity.PropertyAnimationActivity"/>
        <activity android:name=".activity.HeartViewActivity"/>
        <activity android:name=".activity.TextTestActivity"  android:hardwareAccelerated="false"/>
        <activity android:name=".activity.XfermodesActivity"/>
        <activity android:name=".activity.DrawBoardActivity" android:hardwareAccelerated="false"/>
        <activity android:name=".activity.RxBusActivity"/>
    </application>

</manifest>